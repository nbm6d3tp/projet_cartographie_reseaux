<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="20.0" styleClass="tab" stylesheets="@../../../css/global_styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.internetcartographier.controller.leftmenubar.tab.StatisticsTabController">
   <children>
      <VBox>
         <children>
            <Label text="Application">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="text" />
                  <String fx:value="bold-text" />
               </styleClass>
            </Label>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <VBox fx:id="applicationStatisticsContainer" spacing="3.0" />
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="Graph">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="text" />
                  <String fx:value="bold-text" />
               </styleClass>
            </Label>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <VBox fx:id="graphStatisticsContainer" spacing="3.0" />
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="Traceroute">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="text" />
                  <String fx:value="bold-text" />
               </styleClass>
            </Label>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <VBox fx:id="tracerouteStatisticsContainer" spacing="3.0" />
         </children>
      </VBox>
   </children>
</VBox>
